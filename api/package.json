{
  "name": "api",
  "version": "1.0.0",
  "description": "The API for a personal assistant",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prestart": "npm run orm:run-migrations",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "orm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "orm:run-migrations": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:run",
    "orm:revert-migrations": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:revert",
    "orm:changes": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm schema:log",
    "orm:create-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:create",
    "orm:generate-migration": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:generate"
  },
  "author": "Wouter Lindenhof",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.21.0",
    "inversify": "^4.11.1",
    "merge-graphql-schemas": "^1.5.0",
    "moment": "^2.21.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.1",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^5.0.1",
    "typeorm": "^0.1.16",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.12.4",
    "tslint": "^5.9.1"
  }
}
